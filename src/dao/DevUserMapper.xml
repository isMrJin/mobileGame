<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.DevUserMapper">
    <select id="getDevUser" resultType="entity.DevUser">
        SELECT id,devCode,devName,devPassword FROM dev_user WHERE devCode =#{devCode}
    </select>
</mapper>